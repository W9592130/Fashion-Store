    <TextView
        android:id="@+id/textView37"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:text="30"
        android:textColor="#0B0A0A"
        app:layout_constraintEnd_toStartOf="@+id/textView39"
        app:layout_constraintHorizontal_bias="0.292"
